{
  "name": "@hit/feature-pack-dashboard-shell",
  "version": "1.0.128",
  "description": "Dashboard shell with sidebar, topbar, notifications, theme support, ERP UI kit, and table views",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./pages/*": {
      "types": "./dist/pages/*.d.ts",
      "import": "./dist/pages/*.js",
      "require": "./dist/pages/*.js",
      "default": "./dist/pages/*.js"
    },
    "./schema": {
      "types": "./dist/schema/table-views.d.ts",
      "import": "./dist/schema/table-views.js",
      "require": "./dist/schema/table-views.js",
      "default": "./dist/schema/table-views.js"
    },
    "./server/api/*": {
      "types": "./dist/server/api/*.d.ts",
      "import": "./dist/server/api/*.js",
      "require": "./dist/server/api/*.js",
      "default": "./dist/server/api/*.js"
    }
  },
  "files": [
    "dist",
    "feature-pack.yaml",
    "migrations"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "peerDependencies": {
    "@hit/acl-utils": "*",
    "@hit/sdk": "*",
    "@hit/ui-kit": "*",
    "drizzle-orm": "^0.38.0",
    "lucide-react": "^0.460.0",
    "next": "15.5.9",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-grid-layout": "^1.4.4",
    "react-resizable": "^3.0.5"
  },
  "dependencies": {
    "zod": "^4.3.2"
  },
  "devDependencies": {
    "@hit/acl-utils": "file:../hit-acl-utils",
    "@hit/sdk": "file:../../hit-sdks/hit-sdk-typescript",
    "@hit/ui-kit": "file:../hit-ui-kit",
    "@types/node": "^20",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "drizzle-orm": "^0.38.0",
    "lucide-react": "^0.460.0",
    "next": "^15.5.9",
    "react-grid-layout": "^1.4.4",
    "react-resizable": "^3.0.5",
    "typescript": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/c0x65o/hit-feature-pack-dashboard-shell"
  },
  "keywords": [
    "hit",
    "feature-pack",
    "dashboard",
    "shell",
    "navigation",
    "ui-kit"
  ],
  "license": "MIT"
}
